{
    "name": "sadowsm2/gcodeprintserver",
    "description": "Print server for G-Code files",
    "type": "project",
    "license": "GPL v3.0",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marcin Sadowski",
            "email": "msdevouted@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/skeleton": "7.0.*",
        "symfony/asset": "^7.0",
        "symfony/asset-mapper": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/form": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^7.0",
        "doctrine/dbal": "^3",
        "doctrine/orm": "^3.1",
        "symfony/process": "^7.0",
        "symfony/security-bundle": "^7.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.28",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/stimulus-bundle": "^2.16",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/ux-turbo": "^2.16",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/console": "^7.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "symfony/mercure-bundle": "^0.3.8",
        "symfony/apache-pack": "^1.0",
        "symfony/flex": "^2.4"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "symfony/debug-bundle": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/maker-bundle": "^1.58"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
